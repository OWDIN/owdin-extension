{
  "manifest_version": 2,
  "name": "OWDIN Extension",
  "short_name": "OWDIN Extension",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "OWDIN Extension"
  },
  "icons": {
    "16": "app-icon.png",
    "48": "app-icon.png",
    "128": "app-icon.png"
  },
  "web_accessible_resources": [
    "js/*",
    "html/*",
    "css/*",
    "img/*"
  ],
  "permissions": [
    "storage",
    "activeTab"
  ]
}
